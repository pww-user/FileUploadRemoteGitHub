{
  "type": "object",
  "properties": {
    "isFileOpen": {
      "type": "identifier",
      "control": "url.https://encodable.com/uploaddemo/files/"
    },
    "content": {
      "type": "string",
      "control": "property.innerHTML",
      "selector": "body"
    }
  },
  "connect": [
    {
      "match": [
        "isFileOpen"
      ],
      "event": "fileopen"
    }
  ],
  "id": "fileopen"
}